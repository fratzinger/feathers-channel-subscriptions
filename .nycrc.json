{
  "extends": "@istanbuljs/nyc-config-typescript",
  "verbose": false,
  "extension": [
    ".ts",
    ".tsx",
    ".js"
  ],
  "exclude": [
    "**/test/*",
    "**/dist/*",
    "**/*.dist.js",
    "**/tests/*"
  ],
  "print": "detail",
  "reporter": [
    "html",
    "text",
    "text-summary",
    "lcov"
  ],
  "watermarks": {
    "statements": [70, 90],
    "lines": [70, 90],
    "functions": [70, 90],
    "branches": [70, 90]
  }
}